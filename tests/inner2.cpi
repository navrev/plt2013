int main ( ){
   int v1 [5];
   int v2 [5];
   int sum;
   int n;
   int *vector1;
   int *vector2;
 
   sum = 0;
   n = 5;

   v1[0] = 23;
   v1[1] = 79;
   v1[2] = 83;
   v1[3] = 67;
   v1[4] = 53;

   v2[0] = 56;
   v2[1] = 71;
   v2[2] = 84;
   v2[3] = 76;
   v2[4] = 74;

   vector1 = &v1[0];
   vector2 = &v2[0];

   while ( n ){
        sum = sum + (*vector1 * *vector2);
        vector1 = vector1 + 1;
        vector2 = vector2 + 1;
        n = n - 1;
   }
   return  sum;
}

