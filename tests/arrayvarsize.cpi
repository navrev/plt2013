
#include <stdio.h>

int fun(int n) {
  int c;
  int a[n];
  char b[n];
  int sum;
  c = 0;
  sum =0;


  while(c < n) {
    a[c] = c;
    b[c] = 'a' + c;
    c = c+ 1;
  }

  c = 0;
  while(c < n) {
    printf("a[%d] = %d\t",c,a[c]);
    printf("b[%d] = %c\n",c,b[c]);
    c = c+ 1;
    sum = sum + a[c];
  }
  return sum;
}


int main() {
  return fun(10);
}


